    "dotenv": "^8.1.0",
    "pino": "^5.13.6",
    "pg": "^7.12.1",
    "accepts": "^1.3.7"
  },
  "devDependencies": {
    "@hapi/shot": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "pino-pretty": "^3.2.2",
    "standard": "^14.3.1",
    "tap": "^14.6.7"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "boltzmann.js"
    ]
  },
  "repository": {
