{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "an example boltzmann api backed by postgres",
  "main": "boltzmann.js",
  "scripts": {
    "pretest": "docker-compose up -d",
    "test": "tap test",
    "posttest": "npm run lint",
    "lint": "standard",
    "prestart": "docker-compose up -d",
    "start": "node -r dotenv/config boltzmann.js"
  },
  "dependencies": {
    "are-we-dev": "^1.0.0",
    "culture-ships": "^1.0.0",
    "find-my-way": "^2.2.1",
